

node {
  def remote = [:]
  remote.name = 'k8s-master'
  remote.host = '192.168.0.26'
  remote.user = 'root'
  remote.password = 'password'
  remote.allowAnyHosts = true
  stage('Remote SSH') {
    sshCommand remote: remote, command: "cd app2/app2"
    sshCommand remote: remote, command: " pwd "
  }
}